	cd include; make install
	cd lib; make install
	cd api; make install
	cd qi; make install
	cd phquery; make install
	cd ph; make install
	cd util; make install

depend:
	cd include; make depend
	cd lib; make depend
	cd api; make depend
	cd qi; make depend
	cd phquery; make depend
	cd ph; make depend
	cd util; make depend

clean:
	rm -f tags
	cd include; make clean
	cd lib; make clean
	cd api; make clean
	cd qi; make clean
	cd phquery; make clean
	cd ph; make clean
	cd util; make clean

tape:
	cd include; make tape
	cd lib; make tape
	cd api; make tape
	cd qi; make tape
	cd phquery; make tape
	cd ph; make tape
	cd util; make tape

rcs:
	cd include; make rcs
	cd lib; make rcs
	cd api; make rcs
	cd qi; make rcs
	cd phquery; make rcs
	cd ph; make rcs
	cd util; make rcs

tags:
	cd lib; ctags -t -f ../tags ../*/*.[chly]
	ln tags include/tags
	ln tags lib/tags
	ln tags api/tags
	ln tags qi/tags
	ln tags phquery/tags
	ln tags ph/tags
	ln tags util/tags
